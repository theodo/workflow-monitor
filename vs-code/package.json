{
	"name": "casper",
	"license": "MIT",
	"displayName": "Caspr",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.caspr.red",
		"onCommand:extension.caspr.yellow",
		"onCommand:extension.caspr.reset",
		"onCommand:extension.caspr.test"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [{
			"command": "extension.caspr.red",
			"title": "Caspr: Red Alert"
		}, {
			"command": "extension.caspr.yellow",
			"title": "Caspr: Yellow Alert"
		}, {
			"command": "extension.caspr.reset",
			"title": "Caspr: reset config"
		}, {
			"command": "extension.caspr.test",
			"title": "Caspr: test"
		}]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"tslint": "^5.8.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	}
}
